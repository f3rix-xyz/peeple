{
  "name": "peeple-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix \"src/**/*.{ts,js,mjs}\"",
    "build": "pnpm exec tsc",
    "dev": "node --env-file .env dist/src/server.js",
    "chat": "node --env-file .env dist/src/chat.js",
    "start": "docker run -p 3000:3000 --env-file .env api_image",
    "format": "prettier --write \"**/*.{ts,js,mjs,js,md,json,yaml}\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.2",
    "@types/nodemailer": "^6.4.16",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "@neondatabase/serverless": "^0.9.5",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "uuid": "^10.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
